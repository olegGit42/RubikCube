<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-lazy-init="true">

	<bean id="brickW" class="cube.BrickCenter">
		<constructor-arg value="WHITE" name="color1" />
	</bean>

	<bean id="brickR" class="cube.BrickCenter">
		<constructor-arg value="RED" name="color1" />
	</bean>

	<bean id="brickG" class="cube.BrickCenter">
		<constructor-arg value="GREEN" name="color1" />
	</bean>

	<bean id="brickO" class="cube.BrickCenter">
		<constructor-arg value="ORANGE" name="color1" />
	</bean>

	<bean id="brickB" class="cube.BrickCenter">
		<constructor-arg value="BLUE" name="color1" />
	</bean>

	<bean id="brickY" class="cube.BrickCenter">
		<constructor-arg value="YELLOW" name="color1" />
	</bean>

</beans>
