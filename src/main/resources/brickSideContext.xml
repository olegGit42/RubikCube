<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-lazy-init="true">

	<bean id="brickWO" class="cube.BrickSide">
		<constructor-arg value="WHITE" name="color1" />
		<constructor-arg value="ORANGE" name="color2" />
	</bean>

	<bean id="brickWB" class="cube.BrickSide">
		<constructor-arg value="WHITE" name="color1" />
		<constructor-arg value="BLUE" name="color2" />
	</bean>

	<bean id="brickWR" class="cube.BrickSide">
		<constructor-arg value="WHITE" name="color1" />
		<constructor-arg value="RED" name="color2" />
	</bean>

	<bean id="brickWG" class="cube.BrickSide">
		<constructor-arg value="WHITE" name="color1" />
		<constructor-arg value="GREEN" name="color2" />
	</bean>

	<bean id="brickYO" class="cube.BrickSide">
		<constructor-arg value="YELLOW" name="color1" />
		<constructor-arg value="ORANGE" name="color2" />
	</bean>

	<bean id="brickYG" class="cube.BrickSide">
		<constructor-arg value="YELLOW" name="color1" />
		<constructor-arg value="GREEN" name="color2" />
	</bean>

	<bean id="brickYR" class="cube.BrickSide">
		<constructor-arg value="YELLOW" name="color1" />
		<constructor-arg value="RED" name="color2" />
	</bean>

	<bean id="brickYB" class="cube.BrickSide">
		<constructor-arg value="YELLOW" name="color1" />
		<constructor-arg value="BLUE" name="color2" />
	</bean>

	<bean id="brickGO" class="cube.BrickSide">
		<constructor-arg value="GREEN" name="color1" />
		<constructor-arg value="ORANGE" name="color2" />
	</bean>

	<bean id="brickGR" class="cube.BrickSide">
		<constructor-arg value="GREEN" name="color1" />
		<constructor-arg value="RED" name="color2" />
	</bean>

	<bean id="brickBO" class="cube.BrickSide">
		<constructor-arg value="BLUE" name="color1" />
		<constructor-arg value="ORANGE" name="color2" />
	</bean>

	<bean id="brickBR" class="cube.BrickSide">
		<constructor-arg value="BLUE" name="color1" />
		<constructor-arg value="RED" name="color2" />
	</bean>

</beans>
