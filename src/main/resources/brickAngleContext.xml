<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-lazy-init="true">

	<bean id="brickWGO" class="cube.BrickAngle">
		<constructor-arg value="w" name="color1" />
		<constructor-arg value="g" name="color2" />
		<constructor-arg value="o" name="color3" />
	</bean>

	<bean id="brickWOB" class="cube.BrickAngle">
		<constructor-arg value="w" name="color1" />
		<constructor-arg value="o" name="color2" />
		<constructor-arg value="b" name="color3" />
	</bean>

	<bean id="brickWBR" class="cube.BrickAngle">
		<constructor-arg value="w" name="color1" />
		<constructor-arg value="b" name="color2" />
		<constructor-arg value="r" name="color3" />
	</bean>

	<bean id="brickWRG" class="cube.BrickAngle">
		<constructor-arg value="w" name="color1" />
		<constructor-arg value="r" name="color2" />
		<constructor-arg value="g" name="color3" />
	</bean>

	<bean id="brickYBO" class="cube.BrickAngle">
		<constructor-arg value="y" name="color1" />
		<constructor-arg value="b" name="color2" />
		<constructor-arg value="o" name="color3" />
	</bean>

	<bean id="brickYOG" class="cube.BrickAngle">
		<constructor-arg value="y" name="color1" />
		<constructor-arg value="o" name="color2" />
		<constructor-arg value="g" name="color3" />
	</bean>

	<bean id="brickYGR" class="cube.BrickAngle">
		<constructor-arg value="y" name="color1" />
		<constructor-arg value="g" name="color2" />
		<constructor-arg value="r" name="color3" />
	</bean>

	<bean id="brickYRB" class="cube.BrickAngle">
		<constructor-arg value="y" name="color1" />
		<constructor-arg value="r" name="color2" />
		<constructor-arg value="b" name="color3" />
	</bean>

</beans>
