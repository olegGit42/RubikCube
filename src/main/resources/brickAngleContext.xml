<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-lazy-init="true">

	<bean id="brickWGO" class="cube.BrickAngle">
		<constructor-arg value="WHITE" name="color1" />
		<constructor-arg value="GREEN" name="color2" />
		<constructor-arg value="ORANGE" name="color3" />
	</bean>

	<bean id="brickWOB" class="cube.BrickAngle">
		<constructor-arg value="WHITE" name="color1" />
		<constructor-arg value="ORANGE" name="color2" />
		<constructor-arg value="BLUE" name="color3" />
	</bean>

	<bean id="brickWBR" class="cube.BrickAngle">
		<constructor-arg value="WHITE" name="color1" />
		<constructor-arg value="BLUE" name="color2" />
		<constructor-arg value="RED" name="color3" />
	</bean>

	<bean id="brickWRG" class="cube.BrickAngle">
		<constructor-arg value="WHITE" name="color1" />
		<constructor-arg value="RED" name="color2" />
		<constructor-arg value="GREEN" name="color3" />
	</bean>

	<bean id="brickYBO" class="cube.BrickAngle">
		<constructor-arg value="YELLOW" name="color1" />
		<constructor-arg value="BLUE" name="color2" />
		<constructor-arg value="ORANGE" name="color3" />
	</bean>

	<bean id="brickYOG" class="cube.BrickAngle">
		<constructor-arg value="YELLOW" name="color1" />
		<constructor-arg value="ORANGE" name="color2" />
		<constructor-arg value="GREEN" name="color3" />
	</bean>

	<bean id="brickYGR" class="cube.BrickAngle">
		<constructor-arg value="YELLOW" name="color1" />
		<constructor-arg value="GREEN" name="color2" />
		<constructor-arg value="RED" name="color3" />
	</bean>

	<bean id="brickYRB" class="cube.BrickAngle">
		<constructor-arg value="YELLOW" name="color1" />
		<constructor-arg value="RED" name="color2" />
		<constructor-arg value="BLUE" name="color3" />
	</bean>

</beans>
